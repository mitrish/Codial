<!-- This file contains the partial view for header -->

<header>
    Page header

    <ul>
        <% if (locals.user){%>
            <li>
                <%= user.name%>
                    <a href="/users/profile"></a>
            </li>
            <li>
                <a href="/users/sign-out">Sign Out</a>
            </li>
        <% } else {%>
            <li>
                <a href="/users/sign-in">Sign In</a>
            </li>
            <li>
                <a href="/users/sign-up">Sign Up</a>
            </li>
        <% }%>
    </ul>
</header>
